  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:es6": "npm run compile:es6",
    "dev:modules": "npm run compile:modules",
    "compile": "run-p compile:* && npm run bundle",
    "compile:es5": "tsc --p ./tsconfig-es5.json",
    "compile:es6": "tsc --p ./tsconfig.json",
    "compile:modules": "tsc --p ./tsconfig-modules.json",
    "bundle": "node ./bundler.config.js",
    "preview": "http-server ./docs"
  },
  "devDependencies": {
    "glob": "^7.1.4",
    "http-server": "^0.11.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@codewithkyle/notifyjs": "^0.1.0",
    "animejs": "^3.0.1",
    "uuid": "^3.3.2"
  },
  "webDependencies": [
    {
      "package": "uuid/v4",
      "import": "* as uuid"
    },
    {
      "package": "animejs",
      "import": "anime"
    },
    {
      "package": "@codewithkyle/notifyjs",
      "import": "{ Notify }"
    }
  ]
}
